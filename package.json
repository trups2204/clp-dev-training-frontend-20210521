{
  "name": "flickr-images",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:server": "cd server && ts-node-dev --respawn --transpileOnly ./server.ts && wait-on tcp:5000",
    "start:local": "dotenv -e env/.env.local yarn run start:server",
    "build": "webpack --config webpack.dev.js",
    "test": "react-scripts test --env=jsdom-fourteen",
    "test-coverage": "react-scripts test --coverage --watchAll=false --env=jsdom-fourteen",
    "test-server": "node ./node_modules/.bin/jest --config server/jest.config.js --watchAll",
    "test-server:nowatch": "node ./node_modules/.bin/jest --config server/jest.config.js",
    "lint": "tsc --noEmit && eslint '*/**/*.{ts,tsx}' --quiet --fix"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/*.d.ts",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/serviceWorker.ts",
      "!**/__test_helpers__/**"
    ]
  },
  "pre-commit": [
    "test-coverage",
    "test-server:nowatch"
  ],
  "dependencies": {
    "@testing-library/react-hooks": "^7.0.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "4.17.0",
    "express-interceptor": "^1.2.0",
    "history": "^4.10.1",
    "http-status-codes": "^1.4.0",
    "lodash": "^4.17.21",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^5.10.0",
    "redux-persist-transform-filter": "^0.0.20",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.10.1",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@types/express": "4.17.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux-batched-actions": "^0.1.5",
    "@types/redux-persist-transform-filter": "^0.0.4",
    "@types/request-as-curl": "^0.1.0",
    "@types/supertest": "^2.0.10",
    "@types/webpack": "^4.41.10",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.4.2",
    "dotenv-cli": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.10.1",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "html-webpack-plugin": "^4.0.3",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.4",
    "react-test-renderer": "^16.14.0",
    "style-loader": "^1.1.3",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.2",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "ts-node-dev": "1.0.0-pre.44",
    "tslint": "^6.1.0",
    "typescript": "3.9.6",
    "webpack": "4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  }
}
